<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:javaee="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
  <servlet>
    <servlet-name>logonServlet</servlet-name>
    <servlet-class>mrfault.user.LogonCmd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logonServlet</servlet-name>
    <url-pattern>/logon.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resourceServlet</servlet-name>
    <servlet-class>mrfault.user.CreateResourceCmd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resourceServlet</servlet-name>
    <url-pattern>/createresource.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>faultServlet</servlet-name>
    <servlet-class>mrfault.user.CreateFaultCmd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>faultServlet</servlet-name>
    <url-pattern>/createfault.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deletefaultServlet</servlet-name>
    <servlet-class>mrfault.user.DeleteFaultCmd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deletefaultServlet</servlet-name>
    <url-pattern>/deletefault.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deleteuserServlet</servlet-name>
    <servlet-class>mrfault.user.DeleteUserCmd</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteuserServlet</servlet-name>
    <url-pattern>/deleteuser.do</url-pattern>
  </servlet-mapping>
  <servlet>
	<servlet-name>updateServlet</servlet-name>
	<servlet-class>mrfault.user.UpdateFaultCmd</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>updateServlet</servlet-name>
	<url-pattern>/updatefault.do</url-pattern>
  </servlet-mapping>
</web-app>